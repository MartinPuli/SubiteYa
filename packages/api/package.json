    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.10",
    "openai": "^6.2.0",
    "pg": "^8.16.3",
    "pino": "^10.0.0",
    "prisma": "^5.7.0",
    "redis": "^4.6.11",
    "resend": "^6.4.1",
    "typescript": "^5.3.2",
    "zod": "^4.1.12"
