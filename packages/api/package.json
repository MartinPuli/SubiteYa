    "@paralleldrive/cuid2": "^3.0.4",
    "@prisma/client": "^5.7.0",
    "@subiteya/observability": "workspace:*",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@upstash/qstash": "^2.8.4",
    "axios": "^1.13.2",
    "bullmq": "^5.63.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
