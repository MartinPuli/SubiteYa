{
  "buildCommand": "npm run build --workspace=@subiteya/web",
  "outputDirectory": "packages/web/dist",
  "installCommand": "rm -rf node_modules package-lock.json && npm install",
  "framework": null,
  "build": {
    "env": {
      "NODE_VERSION": "20",
      "ROLLUP_USE_NATIVE": "false"
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
